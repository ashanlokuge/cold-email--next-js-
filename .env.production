# Production Environment Variables for Vercel Deployment
# DO NOT COMMIT ACTUAL VALUES - Use Vercel Environment Variables Instead

# Azure Communication Services - REQUIRED
COMMUNICATION_SERVICES_CONNECTION_STRING=your_azure_communication_services_connection_string_here

# Azure Resource Configuration - REQUIRED
AZ_SUBSCRIPTION_ID=your_azure_subscription_id_here
AZ_RESOURCE_GROUP=your_resource_group_name_here
AZ_EMAIL_SERVICE_NAME=your_email_service_name_here
AZ_EMAIL_DOMAIN=your_verified_domain_here

# Production Authentication - Service Principal
AZURE_AUTH_METHOD=service-principal
AZURE_CLIENT_ID=your_azure_client_id_here
AZURE_CLIENT_SECRET=your_azure_client_secret_here
AZURE_TENANT_ID=your_azure_tenant_id_here

# Optional Settings
RATE_PER_MINUTE=60
JITTER_PCT=20
MAX_RETRIES=3